<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Outer Green Circle -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/white"/> <!-- Green color -->
        </shape>
    </item>

    <!-- Inner White Circle -->
    <item android:top="2dp" android:right="2dp" android:left="3dp" android:bottom="3dp">
        <shape android:shape="oval">
            <solid android:color="#00FF00"/> <!-- White color -->
        </shape>
    </item>

</layer-list>
